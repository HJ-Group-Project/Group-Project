<Window x:Class="BuzzfeedQuiz.spongebobQuiz"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BuzzfeedQuiz"
        mc:Ignorable="d"
        Title="spongebobQuiz" Height="450" Width="800">
    <Grid Background="#FFEDE9D6">
        <GroupBox x:Name="ques1GB" Header="In your free time, you are:" HorizontalAlignment="Left" Height="158" Margin="10,10,0,0" VerticalAlignment="Top" Width="346" FontSize="22" Foreground="#FFF09404" BorderBrush="#FFDCF709" FontWeight="Bold" FontFamily="Lucida Sans Unicode" Background="#FFFCF514">
            <StackPanel x:Name="ques1SP" HorizontalAlignment="Left" Height="139" Margin="-6,4,0,0" VerticalAlignment="Top" Width="336">
                <RadioButton x:Name="musicRB" Content="Listening to music" FontWeight="Normal" FontFamily="Segoe UI"/>
                <RadioButton x:Name="movieRB" Content="Watching movies" FontWeight="Normal" FontFamily="Segoe UI Emoji"/>
                <RadioButton x:Name="hangRB" Content="Hanging out with friends" FontWeight="Normal" FontFamily="Segoe UI Emoji"/>
                <RadioButton x:Name="workRB" Content="Working" FontWeight="Normal" FontFamily="Segoe UI Emoji"/>
            </StackPanel>
        </GroupBox>
        <GroupBox x:Name="ques2GB" Header="Pick a color:" HorizontalAlignment="Left" Height="158" Margin="400,10,0,0" VerticalAlignment="Top" Width="353" FontSize="22" Foreground="#FFF49806" FontWeight="Bold" FontFamily="Lucida Sans Unicode" BorderBrush="#FFFAEA09" Background="#FFFCF514"/>
        <StackPanel x:Name="ques2SP" HorizontalAlignment="Left" Height="122" Margin="400,46,0,0" VerticalAlignment="Top" Width="353">
            <RadioButton x:Name="greenRB" Content="Green" FontSize="22"/>
            <RadioButton x:Name="pinkRB" Content="Pink" FontSize="22"/>
            <RadioButton x:Name="purpRB" Content="Purple" FontSize="22"/>
            <RadioButton x:Name="orangeRB" Content="Orange" FontSize="22"/>
        </StackPanel>
        <GroupBox x:Name="ques3GB" Header="Pick your favorite food:" HorizontalAlignment="Left" Height="178" Margin="10,196,0,0" VerticalAlignment="Top" Width="346" FontSize="22" FontFamily="Lucida Sans Unicode" FontWeight="Bold" Foreground="#FFF7A308" BorderBrush="#FFF7F903" Background="#FFFCF514"/>
        <GroupBox x:Name="ques4GB" Header="Pick a Spongebob quote:" HorizontalAlignment="Left" Height="178" Margin="400,196,0,0" VerticalAlignment="Top" Width="353" FontSize="22" FontFamily="Lucida Sans Unicode" FontWeight="Bold" BorderBrush="#FFF9FA06" Foreground="#FFFCA404" Background="#FFFCF514">
            <StackPanel x:Name="ques4SP" HorizontalAlignment="Left" Height="102" Margin="-2,10,-97,-5.6" VerticalAlignment="Top" Width="563">
                <RadioButton x:Name="garyRB" Content="&quot;Gary, I'm absorbing his blows like I'm made &#xD;&#xA;of some spongey material!&quot;" FontSize="15" FontWeight="Normal" FontStyle="Italic" FontFamily="Segoe UI Emoji"/>
                <RadioButton x:Name="patRB" Content="&quot;I wumbo, you wumbo, he-she-me wumbo&quot;" FontSize="16" FontWeight="Normal" FontStyle="Italic" FontFamily="Segoe UI"/>
                <RadioButton x:Name="plankRB" Content="&quot;Ravioli, ravioli, give me the formuoli&quot;" FontSize="16" FontWeight="Normal" FontFamily="Segoe UI Emoji" FontStyle="Italic"/>
                <RadioButton x:Name="sandRB" Content="&quot;Are shoes from Texas dumb?&quot;" FontSize="16" Height="44" Margin="0,0,0.2,0" FontWeight="Normal" FontStyle="Italic" FontFamily="Segoe UI"/>
            </StackPanel>
        </GroupBox>
        <StackPanel x:Name="ques3SP" HorizontalAlignment="Left" Height="118" Margin="10,232,0,0" VerticalAlignment="Top" Width="346">
            <RadioButton x:Name="crabRB" Content="Crab Cakes" FontSize="22"/>
            <RadioButton x:Name="pizzaRB" Content="Pizza" FontSize="22"/>
            <RadioButton x:Name="spagRB" Content="Spaghetti" FontSize="22"/>
            <RadioButton x:Name="friesRB" Content="Fries" FontSize="22"/>
        </StackPanel>
        <Button x:Name="resultsBTN" Content="Get Results!" HorizontalAlignment="Left" Height="56" Margin="280,355,0,0" VerticalAlignment="Top" Width="235" FontSize="24" Click="ResultsBTN_Click" Foreground="#FF0C0701" BorderBrush="#FF0F0F01" Background="#FFFFB007" FontWeight="Bold"/>

    </Grid>
</Window>
